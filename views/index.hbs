<div class="todo-container">
<form action="/" method="POST">
<h1>To Do List</h1>
<label for="list"></label>
<input type="text"placeholder="Enter" name="list" >
<button class="btn" type="submit" class="signupbtn">Submit</button>
</form>

<ul>
    <li class="todo-list-item">
        {{listItem}}
    </li>
</ul>

<p class="error">{{err}}</p>

</div>


